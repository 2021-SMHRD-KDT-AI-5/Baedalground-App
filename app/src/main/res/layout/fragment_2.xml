<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:clickable="false"
    android:clipChildren="true"
    android:clipToPadding="true"
    tools:context=".Fragment2">

    <TextView
        android:id="@+id/textView"
        android:layout_width="72dp"
        android:layout_height="36dp"
        android:fontFamily="@font/one"
        android:gravity="center"
        android:text="나이"
        android:textColor="#DCF8C838"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline27"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.056"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.457" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="71dp"
        android:layout_height="36dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/one"
        android:gravity="center"
        android:text="성별"
        android:textColor="#DCF8C838"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.001" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="83dp"
        android:layout_height="35dp"
        android:fontFamily="@font/one"
        android:gravity="center"
        android:text="동단위"
        android:textColor="#DCF8C838"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline27"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.083" />

    <CheckBox
        android:id="@+id/cb_4"
        android:layout_width="60dp"
        android:layout_height="24dp"
        android:fontFamily="@font/one"
        android:text="40대"
        android:textSize="12dp"
        android:buttonTint="#DCF8C838"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.153"
        app:layout_constraintStart_toEndOf="@+id/cb_3"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <CheckBox
        android:id="@+id/cb_2"
        android:layout_width="60dp"
        android:layout_height="24dp"
        android:fontFamily="@font/one"
        android:text="20대"
        android:textSize="12dp"
        android:buttonTint="#DCF8C838"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toEndOf="@+id/cb_1"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <CheckBox
        android:id="@+id/cb_1"
        android:layout_width="60dp"
        android:layout_height="24dp"
        android:fontFamily="@font/one"
        android:text="10대"
        android:textSize="12dp"
        android:buttonTint="#DCF8C838"
        app:layout_constraintBottom_toBottomOf="@+id/cb_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/cb_2"
        app:layout_constraintVertical_bias="1.0" />

    <CheckBox
        android:id="@+id/cb_3"
        android:layout_width="60dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/one"
        android:text="30대"
        android:textSize="12dp"
        android:buttonTint="#DCF8C838"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.008"
        app:layout_constraintStart_toEndOf="@+id/cb_2"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="263dp"
        android:layout_height="34dp"
        android:layout_marginBottom="2dp"
        android:orientation="horizontal"


        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5">



        <RadioButton
            android:id="@+id/rb_1"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:fontFamily="@font/one"
            android:text="남자" />



        <RadioButton
            android:id="@+id/rb_2"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/one"
            android:text="여자" />

        <RadioButton
            android:id="@+id/rb_3"
            android:layout_width="wrap_content"
            android:layout_height="33dp"
            android:layout_marginStart="12dp"
            android:checked="false"
            android:clickable="true"
            android:fontFamily="@font/one"
            android:text="상관없음"
            android:textSize="12dp" />

        <CheckBox
            android:id="@+id/cb_"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_weight="1"
            android:fontFamily="@font/one"
            android:text="20대"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.052"
            app:layout_constraintStart_toEndOf="@+id/cb_1"
            app:layout_constraintTop_toTopOf="@+id/textView" />

        <CheckBox
            android:id="@+id/cb_5"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_weight="1"
            android:fontFamily="@font/one"
            android:text="10대"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/cb_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.042"
            app:layout_constraintStart_toEndOf="@+id/textView"
            app:layout_constraintTop_toTopOf="@+id/cb_2"
            app:layout_constraintVertical_bias="1.0" />

        <CheckBox
            android:id="@+id/cb_6"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_marginStart="8dp"
            android:layout_weight="1"
            android:fontFamily="@font/one"
            android:text="30대"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.008"
            app:layout_constraintStart_toEndOf="@+id/cb_2"
            app:layout_constraintTop_toTopOf="@+id/textView" />

        <RadioButton
            android:id="@+id/rb_"
            android:layout_width="wrap_content"
            android:layout_height="33dp"
            android:layout_marginStart="12dp"
            android:layout_weight="1"
            android:checked="false"
            android:clickable="true"
            android:fontFamily="@font/one"
            android:text="상관없음"
            android:textSize="12dp" />

        <CheckBox
            android:id="@+id/cb_7"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_weight="1"
            android:fontFamily="@font/one"
            android:text="40대"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.153"
            app:layout_constraintStart_toEndOf="@+id/cb_3"
            app:layout_constraintTop_toTopOf="@+id/textView" />

        <RadioButton
            android:id="@+id/rb_4"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginStart="8dp"
            android:layout_weight="1"
            android:fontFamily="@font/one"
            android:text="여자" />

        <RadioButton
            android:id="@+id/rb_5"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_weight="1"
            android:fontFamily="@font/one"
            android:text="남자" />

    </RadioGroup>

    <Button
        android:id="@+id/btn_top10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:backgroundTint="#0788C3"
        android:fontFamily="@font/one"
        android:text="TOP 10"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/edt_myaddr"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/edt_myaddr" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.59" />

    <ListView
        android:id="@+id/listview"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline27"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/edt_myaddr"
        android:layout_width="251dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.009" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="290dp"
        android:layout_height="101dp"
        android:src="@drawable/top10logo"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.333" />

</androidx.constraintlayout.widget.ConstraintLayout>