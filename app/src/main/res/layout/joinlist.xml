<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_join_time"
        app:srcCompat="@drawable/chicken" />

    <TextView
        android:id="@+id/tv_join_res"
        android:layout_width="268dp"
        android:layout_height="31dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="100dp"
        android:fontFamily="@font/titleone"
        android:gravity="left|center_vertical"
        android:text="TextView"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/tv_join_check" />

    <TextView
        android:id="@+id/tv_join_menu"
        android:layout_width="0dp"
        android:layout_height="27.5dp"
        android:fontFamily="@font/one"
        android:gravity="center_vertical"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/tv_join_res"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tv_join_res"
        app:layout_constraintTop_toBottomOf="@+id/tv_join_res" />

    <TextView
        android:id="@+id/tv_join_price"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:fontFamily="@font/one"
        android:gravity="center_vertical"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/tv_join_menu"
        app:layout_constraintStart_toStartOf="@+id/tv_join_menu"
        app:layout_constraintTop_toBottomOf="@+id/tv_join_menu" />

    <TextView
        android:id="@+id/tv_join_check"
        android:layout_width="101dp"
        android:layout_height="25dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/one"
        android:gravity="right"
        android:text="TextView"
        android:textColor="#EF0707"
        app:layout_constraintBottom_toTopOf="@+id/tv_join_res"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_join_member"
        android:layout_width="0dp"
        android:layout_height="27.5dp"
        android:fontFamily="@font/one"
        android:gravity="center_vertical"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/tv_join_price"
        app:layout_constraintStart_toStartOf="@+id/tv_join_price"
        app:layout_constraintTop_toBottomOf="@+id/tv_join_price" />

    <TextView
        android:id="@+id/tv_join_time"
        android:layout_width="236dp"
        android:layout_height="23dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/titleone"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>