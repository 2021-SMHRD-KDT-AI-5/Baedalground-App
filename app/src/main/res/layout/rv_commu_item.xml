<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout4"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

  <TextView
      android:id="@+id/tv_list_title"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="15dp"
      android:layout_marginTop="8dp"
      android:hint="commu title"
      android:textSize="18dp"
      android:textStyle="bold"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_chainStyle="packed" />

  <TextView
      android:id="@+id/tv_commu_res"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="5dp"
      android:layout_marginTop="8dp"
      android:hint="commu res"
      app:layout_constraintStart_toStartOf="@+id/tv_list_title"
      app:layout_constraintTop_toBottomOf="@+id/tv_list_title" />

  <TextView
      android:id="@+id/tv_commu_time"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="5dp"
      android:layout_marginBottom="8dp"
      android:hint="commu rvtime"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="@+id/tv_commu_res"
      app:layout_constraintTop_toBottomOf="@+id/tv_commu_res" />

  <TextView
      android:id="@+id/tv_commu_min"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="5dp"
      android:hint="commu min"
      app:layout_constraintEnd_toEndOf="@+id/tv_commu_host_id"
      app:layout_constraintTop_toBottomOf="@+id/tv_commu_host_id" />

  <TextView
      android:id="@+id/tv_commu_host_id"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginEnd="20dp"
      android:hint="commu  host"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="@+id/tv_commu_res" />

  <androidx.constraintlayout.widget.Barrier
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:barrierDirection="top" />
</androidx.constraintlayout.widget.ConstraintLayout>